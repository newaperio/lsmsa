<%- model_class = @concentration.class -%>
<h1><%= @concentration.name.capitalize %> Concentration</h1>



<%= render 'prospies/index' %>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              concentrations_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_concentration_path(@concentration), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              concentration_path(@concentration),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
